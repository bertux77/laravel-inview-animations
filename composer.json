{
  "name": "bertux77/laravel-inview-animations",
  "description": "Laravel Inview Animations: animaciones personalizadas con IntersectionObserver.",
  "keywords": [
    "laravel",
    "animations",
    "scroll",
    "intersectionobserver",
    "css"
  ],
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Alberto Carri√≥n",
      "email": "info@albertofreelance.com",
      "homepage": "https://albertofreelance.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "AlbertoFreelance\\InViewAnimations\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlbertoFreelance\\InViewAnimations\\InViewAnimationsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/bertux77/laravel-inview-animations",
    "source": "https://github.com/bertux77/laravel-inview-animations"
  }
}
